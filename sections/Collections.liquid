<div class="container">
  <div class="row">
    {% for collection in section.blocks %}
        <ul
      class="nav nav-pills mb-3"
      id="pills-tab"
      role="tablist">
     
      <li class="nav-item" role="presentation">
        <button
          class="nav-link"
          id="pills-contact-tab"
          data-bs-toggle="pill"
          data-bs-target="#pills-contact"
          type="button"
          role="tab"
          aria-controls="pills-contact"
          aria-selected="false">Contact</button>
      </li>

    </ul>
    <div class="tab-content" id="pills-tabContent">
      <div
        class="tab-pane fade show active"
        id="pills-contact"
        role="tabpanel"
        aria-labelledby="pills-home-tab">
        test
        </div>
  </div>
    {% endfor %}
    
</div>
</div>

{% schema %}
  {
    "name": "Tab Collection",
    "settings": [],
    "blocks": [
        {
            "type": "collections",
            "name":"collection",
            "settings": [
                {
                    "type": "collection",
                    "id":"collection",
                    "label":"Selection"
                }
            ]
        }
    ],
    "presets": [
      {
        "name": "Tab Collection",
        "blocks": [
            {
                "type": "collections"
            },
            {
                "type": "collections"
            },
            {
                "type": "collections"
            }
        ]
      }
    ]
  }
{% endschema %}